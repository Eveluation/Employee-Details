class Employee{
int age;
String designation;
Employee(int age,String designation)
{
this.age=age;
this.designation=designation;
}
void setAge(){
System.out.println("Age : "+age);
}

void setDesignation(){
System.out.println("Designation : " + designation );
}

}

class Employee_Driver{
public static void main(String[]args){
Employee Emp1=new Employee(35,"Manager");
Employee Emp2=new Employee(25,"Accountant");

Emp1.setAge();
Emp1.setDesignation();
Emp2.setAge();
Emp2.setDesignation();
}
}
